    "must-plugin",
    "sinon"
  ],
  "author": "Johnny Estilles <johnny.estilles@agentia.asia> (http://www.agentia.asia)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/JohnnyEstilles/must-sinon/issues"
  },
  "homepage": "https://github.com/JohnnyEstilles/must-sinon",
  "devDependencies": {
    "eslint": "^0.24.0",
    "istanbul": "^0.3.16",
    "mocha": "^2.2.5",
    "must": "^0.13.0-beta2",
    "pre-commit": "^1.0.10",
    "sinon": "^1.15.4",
    "must": "^0.13.1"
  },
  "dependencies": {
    "oolong": "^1.12.0"
